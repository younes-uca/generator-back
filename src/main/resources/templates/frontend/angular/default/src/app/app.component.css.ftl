#content {
    width: 100%;
    min-height: 100vh;
    padding: 0;
    -webkit-transition: all 0.3s;
    -o-transition: all 0.3s;
    transition: all 0.3s;
}

.wrapper {
    width: 100%;
    display: flex;
    align-items: stretch
}
.btn-circle.btn-lg {
    width: 50px;
    height: 50px;
    padding: 10px 16px;
    font-size: 18px;
    line-height: 1.33;
    border-radius: 25px;
}

.btn-dark{
    background-color: #1b1e21 !important;
    border-color: #4e555b !important;
}

.span {
    margin: auto;
}
.boxShadow{
    box-shadow: -10px 1px 25px -10px black;
}
.navbar-dark{
    background-color: #26272b;
  color: #dee2e6;
    z-index: 100;
}
